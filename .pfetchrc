PF_COLOR=1

# Color of info names:
# Default: unset (auto)
# Valid: 0-9
PF_COL1=

# Color of info data:
# Default: unset (auto)
# Valid: 0-9
PF_COL2=

# Color of title data:
# Default: unset (auto)
# Valid: 0-9
PF_COL3=

# Alignment padding.
# Default: unset (auto)
# Valid: int
PF_ALIGN=

# Which ascii art to use.
# Default: unset (auto)
# Valid: string
PF_ASCII=

